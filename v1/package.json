		"prettier": "prettier --check src preview",
		"prettier:fix": "prettier --write src preview",
		"test": "jest"
	},
	"dependencies": {
		"fastify": "^4.10.2",
		"zod": "^3.20.2"
	},
	"devDependencies": {
		"@guardian/eslint-config-typescript": "^3.0.0",
		"@guardian/prettier": "^2.1.5",
		"@types/jest": "^27.0.0",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.47.1",
		"@typescript-eslint/parser": "^5.47.1",
		"eslint": "^8.30.0",
		"jest": "^27.0.0",
		"pino-pretty": "^9.1.1",
		"prettier": "^2.8.1",
		"ts-jest": "^27.0.0",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.9.4"
	}
}
