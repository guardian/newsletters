{
	"name": "newsletters-api",
	"version": "0.1.0",
	"description": "For actions related to newsletters",
	"private": true,
	"scripts": {
		"dev": "STAGE=DEVELOPMENT ts-node-dev --transpile-only src/index.ts | npx pino-pretty",
		"build": "tsc",
		"lint": "eslint src --ext .js,.jsx,.ts,.tsx",
		"tsc": "tsc",
