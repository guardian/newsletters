{
  "name": "newsletters",
  "version": "0.1.0",
  "description": "newsletters source",
  "repository": "https://github.com/guardian/newsletters.git",
  "author": "The Guardian",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "tsc": "tsc",
    "prebuild": "rm -rf dist cloudformation.yaml",
    "postbuild": "./bin/package.sh",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "prepare": "husky install",
    "test": "jest",
    "upload-artifact": "node-riffraff-artifact"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.6",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@aws-cdk/aws-apigateway": "^1.21.0",
    "@aws-cdk/aws-lambda": "^1.21.0",
    "@aws-cdk/core": "^1.21.0",
    "@guardian/eslint-config": "^0.7.0",
    "@guardian/node-riffraff-artifact": "^0.3.2",
    "@guardian/prettier": "^0.7.0",
    "@types/aws-serverless-express": "^3.3.2",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^5.0.0",
    "aws-cdk": "^1.21.0",
    "aws-sdk": "^2.604.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^7.0.0",
    "jest": "^27.2.5",
    "prettier": "2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "4.4.3"
  }
}
